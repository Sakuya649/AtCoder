        "tori_list = [0] * (n+1)\n",
        "if n >= 3:\n",
        "  tori_list[3] = 1\n",
        "for i in range(4, n+1):\n",
        "  tori_list[i] = (tori_list[i-1] + tori_list[i-2] + tori_list[i-3]) % 10007\n",
        "print(tori_list[n])"
