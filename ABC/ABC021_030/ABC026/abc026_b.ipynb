        "import math\n",
        "r_list = [int(input()) for i in range(int(input()))]\n",
        "r_list.sort()\n",
        "ans = 0\n",
        "for r in range(len(r_list)):\n",
        "  if r % 2 == 1:\n",
        "    ans -= r_list[r] ** 2\n",
        "  else:\n",
        "    ans += r_list[r] ** 2\n",
        "print(abs(ans * math.pi))"
