      "source": [
        "h, w = map(int, input().split())\n",
        "S = [input() for i in range(h)]\n",
        "for i in range(h):\n",
        "  l = \"\"\n",
        "  for j in range(w):\n",
        "    if S[i][j] == \"#\":\n",
        "      l += \"#\"\n",
        "    else:\n",
        "      l+=str(sum([t[max(0,j-1):min(w,j+2)].count(\"#\") for t in S[max(0,i-1):min(h,i+2)]]))\n",
        "  print(l)      "
      ]
    }
  ]
