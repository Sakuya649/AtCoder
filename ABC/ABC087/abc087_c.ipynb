      "source": [
        "n = int(input())\n",
        "A1 = list(map(int, input().split()))\n",
        "A2 = list(map(int, input().split()))\n",
        "total = []\n",
        "for i in range(1, n):\n",
        "  total.append(sum(A1[:i]) + sum(A2[i-1:]))\n",
        "if n == 1:\n",
        "  total.append(A1[0]+A2[0])\n",
        "print(max(total))"
      ]
    }
