      "source": [
        "n = int(input())\n",
        "now, x, y = 0, 0, 0\n",
        "for i in range(n):\n",
        "  t, x1, y1 = map(int, input().split())\n",
        "  t2 = t-now\n",
        "  x2 = abs(x-x1)\n",
        "  y2 = abs(y-y1)\n",
        "  if (x2+y2) > t2 or (x+y+t) % 2 != t2 % 2:\n",
        "    print(\"No\")\n",
        "    break\n",
        "  now = t\n",
        "  x = x1\n",
        "  y = y1\n",
        "else:\n",
        "  print(\"Yes\")"
      ]
    }
