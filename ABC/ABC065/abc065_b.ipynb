        "n = int(input())\n",
        "A = [int(input()) for _ in range(n)]\n",
        "i, count = 0, 1\n",
        "while A[i] != 2:\n",
        "  i = A[i] - 1\n",
        "  count += 1\n",
        "  if count > n:\n",
        "    print(-1)\n",
        "    break\n",
        "else:\n",
        "  print(count)"
