      "source": [
        "#input()**2//4で計算できる\n",
        "a = int(input())\n",
        "ans = 0\n",
        "for i in range(1, a+1):\n",
        "  xy = i * (a-i)\n",
        "  if ans <= xy:\n",
        "    ans = xy\n",
        "print(ans)"
      ]
    }
