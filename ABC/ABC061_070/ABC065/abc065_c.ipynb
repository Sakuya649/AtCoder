      "source": [
        "import math\n",
        "n, m = map(int, input().split())\n",
        "ans = 0\n",
        "if abs(n-m) < 2:\n",
        "  a = math.factorial(n) % (10**9+7)\n",
        "  b = math.factorial(m) % (10**9+7)\n",
        "  if n == m:\n",
        "    ans = 2*a*b % (10**9+7)\n",
        "  else:\n",
        "    ans = a*b % (10**9+7)\n",
        "print(ans)"
      ]
    }
  ]
