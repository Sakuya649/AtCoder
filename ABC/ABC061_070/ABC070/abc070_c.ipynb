        "import math\n",
        "from functools import reduce\n",
        "def lcm(x, y):\n",
        "  return (x * y) // math.gcd(x, y)\n",
        "def lcm_list(numbers):\n",
        "  return reduce(lcm, numbers)\n",
        "n = int(input())\n",
        "T =  [int(input()) for i in range(n)]\n",
        "print(lcm_list(T))"
      ]
