      "source": [
        "n = int(input())\n",
        "A = list(map(int, input().split()))\n",
        "c4, c2, c = 0, 0, 0\n",
        "for a in A:\n",
        "  if a % 4 == 0:\n",
        "    c4 += 1\n",
        "  elif a % 2 == 0:\n",
        "    c2 += 1\n",
        "  else:\n",
        "    c += 1\n",
        "if c4 + 1 == c and n == c4 + c:\n",
        "  print(\"Yes\")\n",
        "elif c4 < c:\n",
        "  print(\"No\")\n",
        "else:\n",
        "  print(\"Yes\")"
      ]
    }
  ]
