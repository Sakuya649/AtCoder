        "N = int(input())\n",
        "num_list = [(i+1) for i in range(2*N+1)]\n",
        "while True:\n",
        "  print(num_list[0], flush=True)\n",
        "  num_list.remove(num_list[0])\n",
        "  num = int(input())\n",
        "  if num == 0:\n",
        "    break\n",
        "  else:\n",
        "    num_list.remove(num)"
      ]
