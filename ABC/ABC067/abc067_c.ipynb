        "n = int(input())\n",
        "A = list(map(int,input().split()))\n",
        "x, y = A.pop(0), sum(A)\n",
        "ans = 10**10\n",
        "for a in A:\n",
        "  if ans > abs(x-y):\n",
        "    ans = abs(x-y)\n",
        "  x, y = x+a, y-a\n",
        "print(ans)"
