        "s = input()\n",
        "s = s + \"$\"\n",
        "num = 1\n",
        "ans = ''\n",
        "for i in range(1, len(s)):\n",
        "  if s[i-1] == s[i]:\n",
        "    num += 1\n",
        "  else:\n",
        "    ans += s[i-1] + str(num)\n",
        "    num = 1\n",
        "print(ans)"
      ]
