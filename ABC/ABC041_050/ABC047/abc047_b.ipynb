        "w, h, n = map(int, input().split())\n",
        "x0, y0 = 0, 0\n",
        "for i in range(n):\n",
        "  x, y, a = map(int, input().split())\n",
        "  if a == 1:\n",
        "    x0 = max(x0, x)\n",
        "  elif a == 2:\n",
        "    w = min(w, x)\n",
        "  elif a == 3:\n",
        "    y0 = max(y0, y)\n",
        "  else:\n",
        "    h = min(h, y)\n",
        "print(max(w-x0, 0) * max(h-y0, 0))"
      ]
    }
