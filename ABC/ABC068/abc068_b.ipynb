      "source": [
        "n = int(input())\n",
        "count_max = 0\n",
        "ans = 1\n",
        "for i in range(1, n+1):\n",
        "  count = 0\n",
        "  j = i\n",
        "  while j%2 == 0:\n",
        "    j /= 2\n",
        "    count += 1\n",
        "  if count_max < count:\n",
        "    count_max = count\n",
        "    ans = i\n",
        "print(ans)"
      ]
    }
