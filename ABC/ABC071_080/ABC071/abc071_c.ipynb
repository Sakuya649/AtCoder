        "import collections\n",
        "n = int(input())\n",
        "A = sorted(list(map(int, input().split())))\n",
        "x, y, i = 0, 0, 1\n",
        "while i < n:\n",
        "  if A[i] == A[i-1]:\n",
        "    x, y = y, A[i]\n",
        "    i += 1\n",
        "  i += 1\n",
        "print(x*y)"
