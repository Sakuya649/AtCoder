        "a, b, k = map(int, input().split())\n",
        "n = max(a, b)\n",
        "for i in range(n, 0, -1):\n",
        "  if a % i == 0 and b % i == 0:\n",
        "    k -= 1\n",
        "    if k == 0:\n",
        "      print(i)\n",
        "      break"
