      "source": [
        "import collections\n",
        "n = int(input())\n",
        "A = list(map(int, input().split()))\n",
        "c = collections.Counter(A)\n",
        "ans = 0\n",
        "for k, v in c.items():\n",
        "  if k < v:\n",
        "    ans += v - k\n",
        "  elif k > v:\n",
        "    ans += v\n",
        "print(ans)"
      ]
    }
  ]
